{
  "name": "@coelho-ui/framer-transitions",
  "version": "1.0.0",
  "description": "A set of framer motion transitions for react",
  "keywords": [
    "framer-transitions"
  ],
  "author": "Raul Semicek Coelho <raul.s.coelho@gmail.com>",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RaulSCoelho/coelho-ui.git",
    "directory": "packages/utilities/framer-transitions"
  },
  "scripts": {
